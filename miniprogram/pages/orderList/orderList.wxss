.info-list .info-list-item .helpbutton {
 background-color: #00B358;
 position: absolute;
 right: 0;
 width: 120rpx;
 height: 240rpx;
 line-height: 240rpx;
 text-align: center;
 color: #fff;
 font-size: 30rpx;
}

.info-list .info-list-item.disabled .helpbutton {
  background-color: #c5c5c5;
}

.info-list .info-list-item .detail {
 height: 80rpx;
 padding: 18rpx 0;
 margin: 0 30rpx 0 30rpx;
}

.info-list .info-list-item .detail  .sub-list{
    height: 52rpx;
}


.info-list .info-list-item .start, .info-list .info-list-item .end {
  color: #333333;
  font-size: 26rpx;
}

.s-icon1 {
  margin: 0 30rpx;
  font-size: 30rpx;
  text-align: left;
  width: 300rpx;
}

.info-list .info-list-item .s-icon2 {
  margin: 0 10rpx 0 100rpx;
  border-color: #f06463;
}

.info-list .info-list-item .s-icon3{
  margin: 0 30rpx 0 400rpx;
  font-size: 35rpx;
  color:#8991da;
  font-weight: 600;
  border-color: #f06463;
}

.info-list .info-list-item .s-icon4{
  margin:0 80rpx;
  border-color: #f06463;
}

.situation{
  width: 400rpx;
  font-size: 35rpx;
  color: rgb(69, 197, 69);
  text-align: right;
  font-weight: 700;
  left: 300rpx;
  right: 0;
}

.add-icon{
  position: fixed;
  width: 42px;
  height: 42px;
  bottom: 30px;
  right: 20px;
}

.front-button1{
  position: fixed;
  height: 100rpx;
  top: 0rpx;
  width: 50%;
  /* float: left;  */
  /* text-align: center; */
  font-size: 40rpx;
  background-color: rgb(221, 206, 206);
}
.front-button1::after{
  border: 0;
}

.front-button2{
  position: fixed;
  height: 100rpx;
  top: 0rpx;
  right: 0rpx;
  width: 50%;  
  float: right;  
  text-align: center;
  font-size: 40rpx;
  /* padding-top: 8rpx; */
  background-color: rgb(255, 255, 255);
}
.front-button2::after{
  border: 0;
}

.open-data-userAvatarUrl {
  width: 150rpx;
  height: 150rpx;
  display: block;
  overflow: hidden;
  border-radius: 60%;
  margin: 50rpx auto;
  float: left;
}